    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, b, c, d" type="Integer" array="False" size=""/>
            <output expression="&quot;Enater the value of a&quot;" newline="True"/>
            <input variable="a"/>
            <output expression="&quot;Enater the value of b&quot;" newline="True"/>
            <input variable="b"/>
            <output expression="&quot;Enter the value of c&quot;" newline="True"/>
